{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "background": "white",
  "padding": 5,
  "height": 200,
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "data/population.json",
      "format": {"type": "json"}
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "joinaggregate",
          "as": ["lower_box_people", "upper_box_people"],
          "ops": ["q1", "q3"],
          "fields": ["people", "people"],
          "groupby": ["age"]
        }
      ]
    },
    {
      "name": "data_1",
      "source": "data_0",
      "transform": [
        {
          "type": "filter",
          "expr": "(datum[\"people\"] < datum[\"lower_box_people\"] - 1.5 * (datum[\"upper_box_people\"] - datum[\"lower_box_people\"])) || (datum[\"people\"] > datum[\"upper_box_people\"] + 1.5 * (datum[\"upper_box_people\"] - datum[\"lower_box_people\"]))"
        },
        {
          "type": "filter",
          "expr": "isValid(datum[\"people\"]) && isFinite(+datum[\"people\"])"
        }
      ]
    },
    {
      "name": "data_2",
      "source": "data_0",
      "transform": [
        {
          "type": "filter",
          "expr": "(datum[\"lower_box_people\"] - 1.5 * (datum[\"upper_box_people\"] - datum[\"lower_box_people\"]) <= datum[\"people\"]) && (datum[\"people\"] <= datum[\"upper_box_people\"] + 1.5 * (datum[\"upper_box_people\"] - datum[\"lower_box_people\"]))"
        },
        {
          "type": "aggregate",
          "groupby": ["age"],
          "ops": ["min", "max", "min", "max"],
          "fields": [
            "people",
            "people",
            "lower_box_people",
            "upper_box_people"
          ],
          "as": [
            "lower_whisker_people",
            "upper_whisker_people",
            "lower_box_people",
            "upper_box_people"
          ]
        }
      ]
    },
    {
      "name": "data_3",
      "source": "source_0",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["age"],
          "ops": ["q1", "q3", "median", "min", "max"],
          "fields": ["people", "people", "people", "people", "people"],
          "as": [
            "lower_box_people",
            "upper_box_people",
            "mid_box_people",
            "min_people",
            "max_people"
          ]
        }
      ]
    }
  ],
  "signals": [
    {"name": "x_step", "value": 20},
    {"name": "width", "update": "bandspace(domain('x').length, 0, 0) * x_step"}
  ],
  "marks": [
    {
      "name": "layer_0_layer_0_marks",
      "type": "symbol",
      "style": ["point", "boxplot-outliers"],
      "from": {"data": "data_1"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "fill": {"value": "transparent"},
          "stroke": {"value": "#4c78a8"},
          "tooltip": {"signal": "format(datum[\"year\"], \"\")"},
          "x": {"scale": "x", "field": "age", "band": 0.5},
          "y": {"scale": "y", "field": "people"}
        }
      }
    },
    {
      "name": "layer_0_layer_1_layer_0_marks",
      "type": "rule",
      "style": ["rule", "boxplot-rule"],
      "from": {"data": "data_2"},
      "encode": {
        "update": {
          "stroke": {"value": "black"},
          "tooltip": {
            "signal": "{\"Upper Whisker of people\": format(datum[\"upper_whisker_people\"], \"\"), \"Lower Whisker of people\": format(datum[\"lower_whisker_people\"], \"\"), \"age\": isValid(datum[\"age\"]) ? datum[\"age\"] : \"\"+datum[\"age\"]}"
          },
          "x": {"scale": "x", "field": "age", "band": 0.5},
          "y": {"scale": "y", "field": "lower_whisker_people"},
          "y2": {"scale": "y", "field": "lower_box_people"}
        }
      }
    },
    {
      "name": "layer_0_layer_1_layer_1_marks",
      "type": "rule",
      "style": ["rule", "boxplot-rule"],
      "from": {"data": "data_2"},
      "encode": {
        "update": {
          "stroke": {"value": "black"},
          "tooltip": {
            "signal": "{\"Upper Whisker of people\": format(datum[\"upper_whisker_people\"], \"\"), \"Lower Whisker of people\": format(datum[\"lower_whisker_people\"], \"\"), \"age\": isValid(datum[\"age\"]) ? datum[\"age\"] : \"\"+datum[\"age\"]}"
          },
          "x": {"scale": "x", "field": "age", "band": 0.5},
          "y": {"scale": "y", "field": "upper_box_people"},
          "y2": {"scale": "y", "field": "upper_whisker_people"}
        }
      }
    },
    {
      "name": "layer_1_layer_0_marks",
      "type": "rect",
      "style": ["bar", "boxplot-box"],
      "from": {"data": "data_3"},
      "encode": {
        "update": {
          "fill": {"value": "#4c78a8"},
          "tooltip": {
            "signal": "{\"Max of people\": format(datum[\"max_people\"], \"\"), \"Q3 of people\": format(datum[\"upper_box_people\"], \"\"), \"Median of people\": format(datum[\"mid_box_people\"], \"\"), \"Q1 of people\": format(datum[\"lower_box_people\"], \"\"), \"Min of people\": format(datum[\"min_people\"], \"\"), \"age\": isValid(datum[\"age\"]) ? datum[\"age\"] : \"\"+datum[\"age\"]}"
          },
          "xc": {"scale": "x", "field": "age", "band": 0.5},
          "width": {"value": 14},
          "y": {"scale": "y", "field": "lower_box_people"},
          "y2": {"scale": "y", "field": "upper_box_people"}
        }
      }
    },
    {
      "name": "layer_1_layer_1_marks",
      "type": "rect",
      "style": ["tick", "boxplot-median"],
      "from": {"data": "data_3"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "fill": {"value": "white"},
          "tooltip": {
            "signal": "{\"Max of people\": format(datum[\"max_people\"], \"\"), \"Q3 of people\": format(datum[\"upper_box_people\"], \"\"), \"Median of people\": format(datum[\"mid_box_people\"], \"\"), \"Q1 of people\": format(datum[\"lower_box_people\"], \"\"), \"Min of people\": format(datum[\"min_people\"], \"\"), \"age\": isValid(datum[\"age\"]) ? datum[\"age\"] : \"\"+datum[\"age\"]}"
          },
          "xc": {"scale": "x", "field": "age", "band": 0.5},
          "yc": {"scale": "y", "field": "mid_box_people"},
          "width": {"value": 14},
          "height": {"value": 1}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "band",
      "domain": {
        "fields": [
          {"data": "data_1", "field": "age"},
          {"data": "data_2", "field": "age"},
          {"data": "data_3", "field": "age"}
        ],
        "sort": true
      },
      "range": {"step": {"signal": "x_step"}},
      "paddingInner": 0,
      "paddingOuter": 0
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "data_1", "field": "people"},
          {"data": "data_2", "field": "lower_whisker_people"},
          {"data": "data_2", "field": "lower_box_people"},
          {"data": "data_2", "field": "upper_box_people"},
          {"data": "data_2", "field": "upper_whisker_people"},
          {"data": "data_3", "field": "lower_box_people"},
          {"data": "data_3", "field": "upper_box_people"},
          {"data": "data_3", "field": "mid_box_people"}
        ]
      },
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    }
  ],
  "axes": [
    {
      "scale": "y",
      "orient": "left",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "age",
      "labelAlign": "right",
      "labelAngle": 270,
      "labelBaseline": "middle",
      "labelOverlap": true,
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "people",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 0
    }
  ]
}
